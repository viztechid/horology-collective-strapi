{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Watch Catalog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "watch_series": {
      "type": "string",
      "required": true
    },
    "released_year": {
      "type": "date",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "float",
      "unique": false,
      "required": true
    },
    "watch_supporting_images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "watch_main_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text"
    },
    "watch_brands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::watch.watch",
      "inversedBy": "watch_catalogs"
    },
    "criteria": {
      "type": "enumeration",
      "enum": [
        "UNUSED",
        "MINT",
        "NEAR MINT",
        "EXCELLENT+5",
        "AS-IS"
      ]
    },
    "Availability": {
      "type": "boolean",
      "default": true
    }
  }
}
